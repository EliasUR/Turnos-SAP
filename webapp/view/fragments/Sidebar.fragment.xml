<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.f">
    <SideNavigation xmlns="sap.tnt" xmlns:core="sap.ui.core" expanded="{viewModel>/expanded}" 
        id="sideBar" itemSelect="onItemSelect" selectedKey="{viewModel/page}" >
        <NavigationList>
            <NavigationListItem text="Home"  key="Home" icon="home"/>
            <NavigationListItem text="Sacar Turno" key="Nuevo Turno" icon="sap-icon://clinical-order"/>
            <NavigationListItem text="Todos"  key="Medicos" icon="sap-icon://doctor"/>
            <NavigationListItem text="Ver por Especialidad" 
                items="{/EspecialidadSet}" icon="sap-icon://accidental-leave">
                <NavigationListItem xmlns="sap.tnt" text="{Nombre}" key="{IdEspecialidad}"/>
            </NavigationListItem>
        </NavigationList>
        <fixedItem>
            <NavigationList>
            </NavigationList>
        </fixedItem>
    </SideNavigation>
</core:FragmentDefinition>